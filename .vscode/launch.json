{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "mdel",
      "program": "${workspaceFolder}/bin/mdel.js",
      "env": {
        "NODE_ENV": "development",
        "MDEL_MATCH": "rx:*"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "server",
      "program": "${workspaceFolder}/lib/server",
      "outputCapture": "std",
      "env": {
        "NODE_ENV": "development",
        "MQTT_USERNAME": "test",
        "MQTT_PASSWORD": "test"
      }
    }
  ]
}
